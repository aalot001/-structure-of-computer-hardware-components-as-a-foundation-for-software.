#include <stdio.h>
#include <stdlib.h>

int main()
{
int fact = 1,num,i;

printf("Enter the number : ");
scanf("%d", &num);

for(i = 1;i <= num;i++)
{
fact = fact * i;
}

printf("Factorial of %d is %d", num, fact);

return 0;
}

